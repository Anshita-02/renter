            <h4><a href="#">www.renters.com</a></h4>
            <div class="left_sub_content">
              <div class="rent">
                <h1>$4,450 - $8,900</h1>
                <h4>Monthly Rent</h4>
              </div>
              <div class="bed">
                <h1>1-3</h1>
                <h4>Bedrooms</h4>
              </div>
              <div class="baths">
                <h1>1-3</h1>
                <h4>Floor</h4>
              </div>
            </div>
          </div>

